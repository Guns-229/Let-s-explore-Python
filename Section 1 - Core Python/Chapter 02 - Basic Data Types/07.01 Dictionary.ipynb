  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Optimization try 1\n",
    "\n",
    "def dict_copy(src, dest):\n",
    "    for x in src:\n",
    "        dest[x] = src[x]\n",
    "       \n",
    "c = {}\n",
    "dict_copy(a, c)\n",
    "dict_copy(b, c)\n",
    "print(a)\n",
    "print(b)\n",
    "print(c)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Lets check for deep and shallow copy\n",
    "print(id(a[1]), id(c[1]))\n",
    "# So its a shallow copy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "metadata": {
