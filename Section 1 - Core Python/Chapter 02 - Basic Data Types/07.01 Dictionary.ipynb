   "source": [
    "### Getting the items, keys and values:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "items: dict_items([('Dabar', 'Honey'), ('Patanjali', 'Desi Ghee'), ('Pidilite', 'Fevicol')])\n",
